<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Param-based Execution</name>
  <description />
  <settings ref="TestProject_BuildCommitStatusPublisher">
    <parameters />
    <build-runners>
      <runner id="RUNNER_21" name="Execute if Parameter doesn't have value" type="simpleRunner">
        <conditions>
          <matches name="ololo" value="(?:)" />
        </conditions>
        <parameters>
          <param name="script.content" value="echo Empty Parameter" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

