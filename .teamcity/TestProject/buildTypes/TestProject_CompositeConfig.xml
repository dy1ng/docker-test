<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4bad4992-8b38-4a6c-92f4-a5bf184fce20" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>CompositeConfig</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_3" sourceBuildTypeId="TestProject_UpdateSoftwareInfoViaPr" cleanDestination="false">
        <revisionRule name="lastSuccessful" revision="latest.lastSuccessful" />
        <artifact sourcePath="+:software.report.md =&gt; ubuntu/software.report.md" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="TestProject_UpdateSoftwareInfoViaPr">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

