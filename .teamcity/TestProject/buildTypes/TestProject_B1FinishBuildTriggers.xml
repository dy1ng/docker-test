<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>B1. Finish Build triggers</name>
  <description />
  <settings ref="TestProject_SimpleBuildTemplate">
    <disabled-settings>
      <setting-ref ref="TRIGGER_5" />
      <setting-ref ref="TRIGGER_6" />
    </disabled-settings>
    <parameters>
      <param name="teamcity.buildQueue.allowMerging" value="false" />
    </parameters>
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_5" type="buildDependencyTrigger">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="dependsOn" value="TestProject_A1Master" />
        </parameters>
      </build-trigger>
      <build-trigger id="TRIGGER_6" type="buildDependencyTrigger">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="dependsOn" value="TestProject_A2Develop" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="TestProject_A1Master">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="TestProject_A2Develop">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

